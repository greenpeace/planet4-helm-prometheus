apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: prometheus-stack-grafana
  namespace: kube-system
  uid: 8a9ef777-5aa2-418d-94f3-f918fe7950a5
  resourceVersion: '1135706946'
  generation: 5
  creationTimestamp: '2020-11-02T05:44:05Z'
  labels:
    app.kubernetes.io/instance: prometheus-stack
    app.kubernetes.io/managed-by: Helm
    app.kubernetes.io/name: grafana
    app.kubernetes.io/version: 9.5.5
    helm.sh/chart: grafana-6.57.4
  annotations:
    cert-manager.io/cluster-issuer: letsencrypt-prod
    kubernetes.io/ingress.class: nginx
    meta.helm.sh/release-name: prometheus-stack
    meta.helm.sh/release-namespace: kube-system
    nginx.ingress.kubernetes.io/auth-signin: https://nginx-oauth.greenpeace.org/oauth2/p4/start?rd=https://$host$uri
    nginx.ingress.kubernetes.io/auth-url: https://nginx-oauth.greenpeace.org/oauth2/p4/auth
  selfLink: >-
    /apis/networking.k8s.io/v1/namespaces/kube-system/ingresses/prometheus-stack-grafana
status:
  loadBalancer:
    ingress:
      - ip: 34.70.19.44
spec:
  tls:
    - hosts:
        - grafana.p4.greenpeace.org
      secretName: grafana-tls
  rules:
    - host: grafana.p4.greenpeace.org
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: prometheus-stack-grafana
                port:
                  number: 80
